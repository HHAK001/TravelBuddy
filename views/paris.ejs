    <main>
        <section class="py-3 container text-center container">
          <div class="row py-lg-1">
            <div class="col-lg-6 col-md-8 mx-auto">
              <h1 class="fw-light">Paris</h1>
              <p class="lead text-muted">France</p>
            </div>
          </div>
          <div class="col align-self-end">
            <iframe style="border-radius:12px" src="https://open.spotify.com/embed/playlist/6DwL4msUc0G7u3k51YrOvF?utm_source=generator" width="50%" height="80" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture" loading="lazy" ></iframe>
          </div>
         </section>
         
      
